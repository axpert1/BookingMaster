<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activitys.activityss.Splace"
    tools:showIn="@layout/activity_splace">

    <ImageView
        android:id="@+id/splacebariamge"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/splacelogo"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_below="@+id/splacebariamge"
        android:paddingLeft="40dp"
        android:paddingRight="40dp"
        android:src="@drawable/textlogo"/>


</RelativeLayout>
